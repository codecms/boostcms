<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body>



<div class="container-fluid p-t-15">
    <div class="row">
	 <div class="col-md-6">
      <div class="card">
        <div class="card-header"><h4></h4></div>
        <div class="card-body">
          
          <form class="form-horizontal" id="signupForm">
#foreach($column in $columns)
#if($column.columnName != $pk.columnName)

                              <div class="form-group">	
								<label class="col-sm-2 control-label">${column.comments}:</label>
								<div class="col-sm-8">
#if($column.isSelect == '1')
									<input id="${column.attrname}" name="${column.attrname}" class="form-control" type="hidden">
							        <select id="list${column.attrname}" class="form-control" onchange="${column.attrname}Change()">
									   <option value=''>请选择</option>
									</select>
#else
							        <input id="${column.attrname}" name="${column.attrname}" class="form-control" type="text">
#end
								</div>
							</div>
#end
#end

            <div class="form-group">
              <div class="col-md-9 col-md-offset-3">
                <button class="btn btn-primary" type="submit">提交</button>
              </div>
            </div>
          </form>
          
        </div>
      </div>
	  </div>
	  </div>
</div>

	<div th:include="include :: footer"></div>
    <div th:include="include :: validatefooter"></div>
		<script type="text/javascript" src="/js/appjs/${pathName}/${classname}/common.js"></script>
	<script type="text/javascript" src="/js/appjs/${pathName}/${classname}/add.js">
	</script>
</body>
</html>
